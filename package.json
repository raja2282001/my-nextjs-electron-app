    "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:electron\"",
    "dev:next": "cd next && npm run dev",
    "dev:electron": "wait-on tcp:3000 && electron .",
    "build:next": "cd next && npm run build && npm run export",
    "build:electron": "electron-builder",
    "package:win": "npm run build:next && electron-builder --win",
    "package:mac": "npm run build:next && electron-builder --mac"
  },
  "build": {
    "appId": "com.myapp.nextjs.electron",
    "productName": "MyNextJSElectronApp",
    "files": [
      "electron/**/*",
      "next/out/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ]
    },
    "mac": {
      "icon": "assets/icon.icns",
      "target": ["dmg"]
    }
